{
    "version": 2,
    "builds": [
      {
        "src": "app.py",  
        "use": "@vercel/python",
        "config": {
        "maxDuration": 60, 
        "memory": 1024    
      }
      }
    ],
    "routes": [
      {
        "src": "/upload/",
        "dest": "app.py"  
      }
    ]
  }
  